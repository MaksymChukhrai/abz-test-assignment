{
  "name": "abz-test-assignment",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://MaksymChukhrai.github.io/abz-test-assignment/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "normalize.css": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "npx react-scripts start --port 3000 --host localhost",
    "sass": "npx node-sass src/styles/main.scss -o src/styles --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "run": "npm run sass && react-scripts start --port 3000 --host localhost"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "Resources": {
    "Stage": {
      "Type": "AWS::ApiGateway::Stage",
      "Properties": {
        "RestApiId": "<String>"
      }
    }
  },
  "devDependencies": {
    "sass": "^1.72.0"
  }
}
